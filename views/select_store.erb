<h2>Current Stores</h2>
<h4>Click on a Store to add Shoes to it.</h4>
<% if @stores.any?() %>
  <% @stores.each() do |store| %>
    <h3><a href="/select_store/<%= store.id %>/add_shoes"><%= store.name() %></a></h3>
  <% end %>
  <% else %>
    <h3>No Stores</h3>
  <% end %>


  <a class="btn btn-default" href="/">Home</a>
  <a class="btn btn-default" href="/">Back</a>
